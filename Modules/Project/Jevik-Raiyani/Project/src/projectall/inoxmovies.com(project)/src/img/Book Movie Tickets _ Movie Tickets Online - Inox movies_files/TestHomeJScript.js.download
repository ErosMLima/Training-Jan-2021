function checkfeaturesmoviesvisiblity() {
    var Hdnmovies = parseInt($("[id$=hfFeaturedMCount]").val());
    if ((Hdnmovies < (NumbersOf_Featured_Movie_Required + 1)) && Hdnmovies != NaN) {
        $('#divFMLeft').css("display", "none");
        $('#divFMRight').css("display", "none");
    }
    else {
        if (isNaN(Hdnmovies)) {
            $('#divFMRight').css("display", "none");
        }
        else {
            $('#divFMRight').css("display", "block");
        }

        $('#divFMLeft').css("display", "none");
    }

}



function UpcomingMovieTrailerControls(Flag) {

    $.post(getRootURL() + "Handlers/HomePageHandler_Cache.ashx?performupcmgmoviews=True&Pagebatch=" + NumbersofUpRcrequired + "&Flag=" + Flag, function (result) {
        var Data = result.toString();
        if (Flag == "1") {
            if (Data != "" && Data != "[object XMLDocument]") {
                $("[id$=divUMTrailerPaging]").html(Data);
                //  UpcomingMovieTrailerList('divUMTrailerPaging_1', 1, NumbersofRcrequired);
                GetUpcomingMovieTrailerList('divUMTrailerPaging_1', 1, NumbersofUpRcrequired);
            }
            else {
                $("[id$=divNoUMTrailers]").css("display", "block");
                $("[id$=divUMTrailerCont_1]").css("display", "none");
            }
        }
        else {
            if (Data != "" && Data != "[object XMLDocument]") {
                $("[id$=divInoxTrailerPaging]").html(Data);
                //  UpcomingMovieTrailerList('divUMTrailerPaging_1', 1, NumbersofRcrequired);
                GetUpcomingMovieTrailerList('divInoxTrailerPaging_1', 1, NumbersofUpRcrequired);
            }
            else {
                $("[id$=divNoInoxTrailers]").css("display", "block");
                $("[id$=divInoxTrailerCont_1]").css("display", "none");
            }
        }

    });

}

