        }
        else {
            $(current).addClass('cls-temp');
            $(current).find('.cls-seltmv-lft').addClass('cls-seltmv-lft-hover');
            $(current).find('.cls-seltmv-mddl').addClass('cls-seltmv-mddl-hover');
