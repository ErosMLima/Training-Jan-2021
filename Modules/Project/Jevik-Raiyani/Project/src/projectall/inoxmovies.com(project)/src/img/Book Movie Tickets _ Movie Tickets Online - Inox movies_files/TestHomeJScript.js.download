var SelectedDiv = "";
function OnClickCommingSoon(ctlid, ctrl) {
    debugger;
    if (SelectedDiv != ctrl) {
        $('.cls-CommingSoon').removeClass('cls-CommingSoon-selected');
        $('#' + ctrl).addClass('cls-CommingSoon-selected');
        $('.cls-container').removeClass('cls-show-Container').addClass('cls-hide-Container');
        $('#divContainer_' + ctlid).removeClass('cls-hide-Container').addClass("cls-show-Container");
        if (ctlid == "1") {
            ComingSoonMovieLIst('divcmgsoonPaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
        }
        if (ctlid == "2") {
            NowShowingMovieLIst('divNowShowingPaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
        }
        else if (ctlid == "3") {
            OffersMovieLIst('divOffersPaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
        } else if (ctlid == "4") {

            PreBookMovieLIst('divPreBookPaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
        }
        else if (ctlid == "5") {
            NearYouMovieLIst('divnearyouPaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
        }
        else if (ctlid == "6") {
            NextChangeMovieLIst('divnextchangePaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
        }
        else if (ctlid == "7") {
            IMAXMovieLIst('divimaxPaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
        }
        else if (ctlid == "8") {
            IMX4DMovieLIst('divimaxPaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
        }
        else if (ctlid == "9") {
            ScreenXMovieLIst('divimaxPaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
        }
        SelectedDiv = ctrl;
    }
    return false;
}

