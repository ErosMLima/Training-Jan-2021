function toggleByClass(current, className) {

    if ($(current).find('.cls-QuickB').html() != "") {
        ShowPagePopUp(1);
        if ($('.div-seltmv-main').hasClass('cls-temp-check')) {
            $('.div-seltmv-main').not(current).removeClass('cls-temp-check');
            $('.div-seltmv-main').not(current).removeClass('cls-temp');
            $('.div-seltmv-main').not(current).find('.cls-seltmv-lft').removeClass('cls-seltmv-lft-hover');
            $('.div-seltmv-main').not(current).find('.cls-seltmv-mddl').removeClass('cls-seltmv-mddl-hover');
            $('.div-seltmv-main').not(current).find('.cls-seltmv-rgt').removeClass('cls-seltmv-rgt-hover');
            $('.div-seltmv-main').not(current).find('.cls-semovie').removeClass('cls-semovie-hover');
            $('.div-seltmv-main').not(current).find('.cls-seltmv-arrw').removeClass('cls-seltmv-arrw-hover');
            $(current).removeClass('cls-temp-check').addClass('cls-temp-check');
        }
        else {
            $('.div-seltmv-main').removeClass('cls-temp-check');
