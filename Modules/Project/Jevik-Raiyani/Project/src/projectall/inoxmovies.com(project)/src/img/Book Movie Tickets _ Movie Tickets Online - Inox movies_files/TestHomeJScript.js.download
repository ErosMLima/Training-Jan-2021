            else {
                var ChildDivID = $('#divComingSoonContainer').children()[0].id;
                $('#' + ChildDivID).fadeOut();
                NowShowingMovieLIst('divNowShowingPaging_1', 1, NumbersofRcrequiredCmgSoon_Now_Show_Offer)
            }
