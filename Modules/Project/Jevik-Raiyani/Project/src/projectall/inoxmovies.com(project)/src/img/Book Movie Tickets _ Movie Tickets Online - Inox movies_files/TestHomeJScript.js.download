                $('#' + ChildDivID).fadeOut();

                $('#divComingSoonContainer').html(arr[0].toString());
                $('#' + $('#divComingSoonContainer').children()[0].id).fadeIn();
                if (parseInt(arr[1].toString()) <= 5) {
                    $('[id$=divAdRotatorSquareBottom]').hide();
