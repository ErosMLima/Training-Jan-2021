        $('#' + DivID).addClass(SetActiveClass);
        $.post(getRootURL() + "Handlers/HomePageHandler_Cache.ashx?ComingSoonMovies=True&PageNumber=" + PageNo + "&Required=" + NumbersofRcrequiredCmgSoon_Now_Show_Offer, function (data) {

