function GetNextFeaturedMovies() {
    $.ajax({
        type: "POST",
        url: getRootURL() + 'Handlers/FeaturedMovieHandler_Cache.ashx?param=' + $("#ctl00_ContentPlaceHolder1_hfCityID").val(),
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        success: function (data) {
            if (data[0].HtmlData != null & data[0].HtmlData != "" & data[0].HtmlData != "[object XMLDocument]") {

                // $("#ctl00_ContentPlaceHolder1_hfDisFeaturedM").val(data[0].ItemCount);
                $("#ctl00_ContentPlaceHolder1_hfDisFeaturedM").val(NumbersOf_Featured_Movie_Required + 1);

                $('#ctl00_ContentPlaceHolder1_dlFeaturedMovieLIst tr').append(data[0].HtmlData);
                IsCompleteFR = "0";
                $(".cls-featuredDiv").closest(".div-sld-content-marg").hover(
              function () {
                  $(this).closest(".div-sld-content-marg").find(".cls-featuredHoverd").css('display', 'block');

              }, function () {

                  //                   $(this).fadeIn();
                  $(this).closest(".div-sld-content-marg").find(".cls-featuredHoverd").css('display', 'none');
              }
            );
                $("#divFeaturedMovies").css({ 'display': 'block' });
                $("#divFeaturedNoData").css({ 'display': 'none' });
                $("#divFeaturedMovies").fadeIn(30, function () {
                    $(this).css('height', 'auto');
                    return false;
                });
                var totFeaturedMCount = $("#ctl00_ContentPlaceHolder1_hfFeaturedMCount").val();
                var itemCount = $("#ctl00_ContentPlaceHolder1_hfDisFeaturedM").val();


                if (parseInt(positionFR) < parseInt(itemCount - NumbersOf_Featured_Movie_Required)) {
                    $("#divFeaturedMovies").animate({ left: "-=" + leftWidthMin }, "slow");
                    positionFR += NumbersOf_Featured_Movie_Required;
                    if (positionFR >= (itemCount - NumbersOf_Featured_Movie_Required)) {
                        $("#divFMRight").css({ 'display': 'none' });
                    }
                }

                if (flag == 1) {
                    NumbersOf_Featured_Movie_Required = 1;
                }

            }
            else {
                debugger;
                $("#divFeaturedMovies").css({ 'display': 'none' });
                $("#divFeaturedNoData").text("No Featured Content Found.");
                $("#divFeaturedNoData").css({ 'display': 'block' });
                $(".cls-features-movies-area").css({ 'display': 'none' });
                $(".cls-me").css({ 'display': 'none' });
            }
        }
    });
}

