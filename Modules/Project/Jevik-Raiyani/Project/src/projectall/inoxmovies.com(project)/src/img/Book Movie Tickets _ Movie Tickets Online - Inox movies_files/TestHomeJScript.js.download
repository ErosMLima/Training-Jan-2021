var positionFR = NumbersOf_Featured_Movie_Required;
var IsCompleteFR = "0";
var num = 0;
function MoveRight() {
    num = num + 1;
    //debugger 
    var totFeaturedMCount = $("#ctl00_ContentPlaceHolder1_hfFeaturedMCount").val();
    var itemCount = parseInt($("#ctl00_ContentPlaceHolder1_hfDisFeaturedM").val());
    //    if(num%2==0 && num!=0)
    //    {
    //    leftWidthMin=parseInt(leftWidthMin)- parseInt(3);
    //    }
    //    else
    //    {
    //    leftWidthMin=parseInt(leftWidthMin)- parseInt(3);
    //    }
    if (parseInt(itemCount) < parseInt(totFeaturedMCount)) {

        if (parseInt(positionFR) < parseInt(itemCount - NumbersOf_Featured_Movie_Required)) {
            if (IsCompleteFR == "0")
                IsCompleteFR == "1";

            $("#divFeaturedMovies").stop(true, true).animate({ left: "-=" + leftWidthMin }, "slow");
            $("#ctl00_ContentPlaceHolder1_hfDisFeaturedM").val(parseInt(itemCount) + 1);
            positionFR += NumbersOf_Featured_Movie_Required;
            IsCompleteFR = "0";
            //GetNextFeaturedMovies();
        }
        else {

            if (parseInt(itemCount) < parseInt(totFeaturedMCount)) {
                if (IsCompleteFR == "0") {
                    IsCompleteFR = "1";
                    $("#divFeaturedMovies").stop(true, true).animate({ left: "-=" + leftWidthMin }, "slow");
                    $("#ctl00_ContentPlaceHolder1_hfDisFeaturedM").val(parseInt(itemCount) + 1);
                    // GetNextFeaturedMovies(); // this function calls ashx page for generating next set of upcoming movies
                    IsCompleteFR = "0";
                    positionFR += NumbersOf_Featured_Movie_Required;
                }
            }
        }
    }
    else {
        //        if (parseInt(positionFR) < parseInt(itemCount - NumbersOf_Featured_Movie_Required)) {
        if (itemCount == totFeaturedMCount) {
            $("#divFeaturedMovies").stop(true, true).animate({ left: "-=" + leftWidthMin }, "slow");
            positionFR += NumbersOf_Featured_Movie_Required;
            $("#ctl00_ContentPlaceHolder1_hfDisFeaturedM").val(parseInt(itemCount) + 1);
            if (positionFR >= (itemCount - NumbersOf_Featured_Movie_Required)) {
                $("#divFMRight").css({ 'display': 'none' });
            }
        }
    }
    $("#divFMLeft").css({ 'display': 'block' });
    //leftWidthMin="229";
}

