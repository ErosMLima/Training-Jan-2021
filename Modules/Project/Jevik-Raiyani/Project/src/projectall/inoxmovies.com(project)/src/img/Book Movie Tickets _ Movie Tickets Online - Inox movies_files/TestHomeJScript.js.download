function OnClickTrailors(flag) {
    document.getElementById('ctl00_ContentPlaceHolder1_hfTrailorType').value = flag;
    $('.cls-CommingSoonTrailor').removeClass('cls-CommingSoonTrailor-selected');

    if (flag == "1") {
        $('#dvUpcomingTrailor').addClass('cls-CommingSoonTrailor-selected');
        GetUpcomingMovieTrailerList('divUMTrailerPaging_1', 1, NumbersofUpRcrequired);
        document.getElementById('divUMTrailerContainer').style.display = "block";
        document.getElementById('divInoxTrailorContainer').style.display = "none";
        document.getElementById('ctl00_ContentPlaceHolder1_divUMTrailerPaging').style.display = "block";
        document.getElementById('ctl00_ContentPlaceHolder1_divInoxTrailerPaging').style.display = "none";
    }
    else {
        $('#dvInoxTrailor').addClass('cls-CommingSoonTrailor-selected');
        GetUpcomingMovieTrailerList('divInoxTrailerPaging_1', 1, NumbersofUpRcrequired);
        document.getElementById('divUMTrailerContainer').style.display = "none";
        document.getElementById('divInoxTrailorContainer').style.display = "block";
        document.getElementById('ctl00_ContentPlaceHolder1_divUMTrailerPaging').style.display = "none";
        document.getElementById('ctl00_ContentPlaceHolder1_divInoxTrailerPaging').style.display = "block";
    }
}
