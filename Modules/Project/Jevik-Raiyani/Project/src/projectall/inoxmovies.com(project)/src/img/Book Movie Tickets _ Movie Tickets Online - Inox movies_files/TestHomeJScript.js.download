function ShowPagePopUp(flag) {
    if (flag == 1) {
        $('.MessagePopupBackGround').addClass('cls-close');
        document.getElementById('divMessagePopupBackGround').style.display = 'block';

        $(".cls-close").click(function () {
            $('.MessagePopupBackGround').removeClass('cls-close');
            document.getElementById('divMessagePopupBackGround').style.display = 'none';
            $('.cls-QuickB').slideUp();
            ClearSelection();
        });
    }
    else {
        $('.MessagePopupBackGround').removeClass('cls-close');
        document.getElementById('divMessagePopupBackGround').style.display = 'none';
        $('.cls-QuickB').slideUp();
        ClearSelection();
    }
}

